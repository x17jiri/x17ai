{
	"CodeGPT.apiKey": "CodeGPT Plus Beta",
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
	},
	"workbench.colorCustomizations": {
		"diffEditor.removedTextBackground": "#ff000050",
		"diffEditor.removedLineBackground": "#ff000010",
		"diffEditor.removedTextBorder": "#ff0000",
		"diffEditor.insertedTextBackground": "#ff000050",
		"diffEditor.insertedLineBackground": "#ff000020",
		"diffEditor.insertedTextBorder": "#ff0000",
		"editorIndentGuide.activeBackground1": "#ffd040",
		"editorInlayHint.foreground": "#909090",
		"editorInlayHint.background": "#0000"
	},
    "rust-analyzer.semanticHighlighting.operator.specialization.logicalNot.enable": true,
    "rust-analyzer.semanticHighlighting.operator.specialization.enable": true,
    //    "rust-analyzer.semanticHighlighting.operator.enable" : false,
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "logical": {
                "foreground": "#8F08C4", // "#808",
            },
            "logicalNot": {
                //                "foreground": "#808",
                "foreground": "#8F08C4", // "#e90",
                //                "fontStyle": "bold",
            },
            "operator.controlFlow": {
                "foreground": "#8F08C4",
                //                "fontStyle": "bold",
            },
			/*
			"entity.name.function.preprocessor": {
				"foreground": "#8A1BFF",
			},
			*/
        }
    },
}